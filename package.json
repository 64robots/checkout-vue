{
  "name": "vue-checkout",
  "author": "64Robots",
  "license": "MIT",
  "version": "0.1.34",
  "description": "Set of customizable components made with Tailwindcss",
  "main": "dist/vue-checkout.ssr.js",
  "browser": "dist/vue-checkout.esm.js",
  "module": "dist/vue-checkout.esm.js",
  "unpkg": "dist/vue-checkout.min.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "node_modules/.bin/eslint ./",
    "serve": "vue-cli-service serve src/dev.js",
    "build": "cross-env NODE_ENV=production concurrently \"rollup --config build/rollup.config.js\" \"npm run build:css\"",
    "build:css": "cross-env NODE_ENV=production tailwindcss build src/assets/app.css -o dist/index.css",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/64robots/vue-checkout"
  },
  "bugs": {
    "url": "https://github.com/64robots/vue-checkout/issues"
  },
  "homepage": "https://github.com/64robots/vue-checkout#readme",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@rollup/plugin-alias": "^3.1.0",
    "@rollup/plugin-babel": "^5.0.0",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-image": "^2.0.5",
    "@rollup/plugin-json": "^4.0.3",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@rollup/plugin-replace": "^2.3.2",
    "@vue/cli-service": "^4.3.1",
    "concurrently": "^5.2.0",
    "cross-env": "^7.0.2",
    "eslint": "^5.13.0",
    "minimist": "^1.2.5",
    "rollup": "^2.7.3",
    "rollup-plugin-terser": "^4.0.4",
    "rollup-plugin-vue": "4.7.1",
    "vue": "^2.6.11",
    "vue-template-compiler": "^2.6.4",
    "vuepress": "^1.4.1"
  },
  "peerDependencies": {
    "vue": "^2.6.11"
  },
  "engines": {
    "node": ">=8"
  },
  "dependencies": {
    "@tailwindcss/custom-forms": "^0.2.1",
    "axios": "^0.19.2",
    "lodash": "^4.17.15",
    "tailwindcss": "^1.4.6",
    "vuelidate": "^0.7.5"
  }
}
